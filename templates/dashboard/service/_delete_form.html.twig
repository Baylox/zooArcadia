<form method="post" action="{{ path('dashboard_service_delete', {'id': service.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer le service {{ service.nom }} ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ service.id) }}">
    <button class="btn btn-dark">Supprimer</button>
</form>

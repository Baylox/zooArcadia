<form method="post" action="{{ path('dashboard_alimentation_delete', {'id': alimentation.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet élément ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ alimentation.id) }}">
    <button class="btn btn-dark">Supprimer</button>
</form>

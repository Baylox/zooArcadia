<form method="post" action="{{ path('dashboard_rapport_delete', {'id': rapport.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer ce rapport ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ rapport.id) }}">
    <button class="btn btn-dark">Supprimer</button>
</form>
